    "typescript": "~2.2.0",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/material": "^2.0.0-beta.10",
    "angular-cropperjs": "^0.1.3",
    "lodash": "^4.17.4",
    "ng2-file-upload": "^1.2.1"
